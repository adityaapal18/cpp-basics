#include<bits/stdc++.h>
using namespace std;
int main()
{
    //switch case for printing corresponding day according to number
    //eg -> 1 for Monday ,7 for sunday and so on.
    int day;
    cin>>day;
    switch (day)
    {
    case 1:
    cout<<"Monday";
        break;
    case 2:
    cout<<"Tuesday";
    break;
    case 3:
    cout<<"Wednesday";
    break;
    case 4:
    cout<<"Thursday";
    break;
    case 5:
    cout<<"Friday";
    break;
    case 6:
    cout<<"Saturday";
    break;
    case 7:
    cout<<"Sunday";
    break;
    default:
    cout<<"Day dosen't falls in any calendar";
    return 0;
    }
}
